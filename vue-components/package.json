{
  "name": "vue-nrtk_explorer",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/nrtk_explorer.umd.js",
  "unpkg": "./dist/nrtk_explorer.umd.js",
  "module": "./dist/nrtk_explorer.mjs",
  "exports": {
    ".": {
      "import": "./dist/nrtk_explorer.mjs",
      "require": "./dist/nrtk_explorer.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@timohausmann/quadtree-ts": "^2.0.0-beta.1",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "scatter-gl": "^0.0.13",
    "semantic-release": "19.0.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.12",
    "vue": "^3.0.0",
    "vue-tsc": "^1.8.6"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
}
